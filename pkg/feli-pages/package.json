{
	"name": "feli-pages",
	"description": "html-pages for documenting feli",
	"version": "0.5.0",
	"private": true,
	"repository": {
		"type": "git",
		"url": "git+https://github.com/charlyoleg2/feli_mono.git"
	},
	"homepage": "https://charlyoleg2.github.io/feli_mono/",
	"author": "charlyoleg2",
	"license": "ISC",
	"keywords": [
		"web-app",
		"web-ui",
		"front-end",
		"local installation"
	],
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run",
		"ci": "run-s build lint check test",
		"github-pages-fix": "touch build/.nojekyll",
		"clean": "rimraf .svelte-kit build node_modules static/pgdsvg"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^3.0.0",
		"globals": "^16.0.0",
		"mdsvex": "^0.12.3",
		"npm-run-all2": "^8.0.4",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"rimraf": "^6.0.1",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"vite": "^6.2.6",
		"vitest": "^3.2.3"
	}
}
